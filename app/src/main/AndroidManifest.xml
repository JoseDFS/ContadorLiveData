<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.contadorlive">

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_action_ball"
            android:label="@string/app_name"
            android:roundIcon="@drawable/ic_action_ball"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Activities.PartidoActivity">
        </activity>
        <activity android:name=".Activities.NewPartidoActivity">
        </activity>
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>